    "prettier": "^3.6.2",
    "svelte": "^5.45.2",
    "svelte-check": "^4.3.4",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@tauri-apps/plugin-fs": "^2.4.5",
    "jspdf": "^4.2.0",
    "jszip": "^3.10.1",
